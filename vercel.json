{
  "version": 2,
  "routes": [
    {
      "src": "/uploads/(.*)",
      "dest": "/uploads/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "buildCommand": "npm run build",
  "installCommand": "npm ci",
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}